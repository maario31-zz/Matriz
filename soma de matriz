#include <stdio.h>
#include <stdlib.h>

int main(){

	int a[3][3], b[3][3], c[3][3], i,j;

	for(i=0;i<3;i++){
					 for(j=0;j<3;j++){
					 				  printf("digite: I:%d, J:%d:\n", i,j);
					 				  scanf("%d", &a[i][j]);
									  }
				  }
printf("#################\n");
	for(i=0;i<3;i++){
					 for(j=0;j<3;j++){
					 				  printf("digite: I:%d, J:%d:\n", i,j);
					 				  scanf("%d", &b[i][j]);
									   }
			   }

 	for(i=0;i<3;i++){
					 for(j=0;j<3;j++){
				  	 				  c[i][j] = a[i][j] + b[i][j];
										 }
										 }
										 printf("#################\n");
  	for(i=0;i<3;i++){
					 for(j=0;j<3;j++){
					 				  printf("%d \t",c[i][j]);
									   }
			   printf("\n");
									   }

system("pause");
return 0;
}

