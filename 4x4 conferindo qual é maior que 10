#include <stdio.h>
int main(){
int m[4][4], i,j,maior=0;
for(i=0;i<4;i++){
    for(j=0;j<4;j++){
        printf("Digite o valor da matriz I:%d e J:  \n: ",i,j);
        scanf("%d", &m[i][j]);
        if(m[i][j]>10){
            maior++;
        }
    }
}
printf("Valores maiores que 10: %d", maior);
return 0;
}
