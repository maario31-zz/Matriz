#include <stdio.h>
int main(){
int m[4][4],i,j;
for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            printf("Entre com seu valor de I%d e J%d: ", i,j);
            scanf("%d", &m[i][j]);
        }
}
for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            printf("\n matriz %d\n", m[i][j]);
        }
        printf("----------------");
}
return 0;
}

