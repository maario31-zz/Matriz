#include <stdio.h>
#include <stdlib.h>

int main(){
int i,j, a[3][3];

for(i=0;i<3;i++){
    for(j=0;j<3;j++){
        printf("Matriz I:%d J:%d:\t", i,j);
        scanf("%d", &a[i][j]);
    }
}
for(i=0;i<3;i++){
    for(j=0;j<3;j++){
    if(i>j){
        a[i][j]=0;
    }
    }
    }
    printf("\n");
    for(i=0;i<3;i++){
    for(j=0;j<3;j++){
        printf("%d \t", a[i][j]);
    }
    printf("\n");}
return 0;
}
