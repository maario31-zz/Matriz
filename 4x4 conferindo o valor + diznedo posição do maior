#include <stdio.h>
int main(){
int m[4][4], i,j,maior=0,i1,j1;
for(i=0;i<4;i++){
    for(j=0;j<4;j++){
        printf("Digite o valor da matriz I:%d e J:%d  \n: ",i,j);
        scanf("%d", &m[i][j]);
        if(m[i][j]>maior){
           maior = m[i][j];
           i1=i;
           j1=j;

        }
    }
}
printf("Valores maiores que 10: %d", maior);
printf("posi√ßao do maior valor I:%d e J:%d", i1, j1);
return 0;
}
